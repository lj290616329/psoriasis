<!--index.wxml-->
<view class="containers">
  <view  bindtap="bindViewTap" class="userinfo">
    <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
    <text class="userinfo-nickname">{{userInfo.nickName}}</text>
  </view>
  <view class="info">
    <view class="action">
        <view style="background:#32c5d2" class="plate" bindtap='toPatient'>
            <view class="message">{{messsge}}</view>
            <image src="/pages/image/ph.png"></image>
            <view class="text">
              <text>我的病人</text>
            
            </view>
        </view>
        <view style="background:#E26A6A" class="plate" bindtap='toIncomplete'>
            <image src="../image/mine.png"></image>
            <view class="text">
              <text>待验证病例</text>
            
            </view>
        </view>
    </view>
    <view class="action">
        <view style="background:#4B77BE" class="plate" bindtap='toPersonal'>
            <image src="../image/data.png"></image>
            <view class="text">
              <text>个人资料</text>
             
            </view>
        </view>
        <view style="background:#8E44AD" class="plate" bindtap='getScancode'>
            <image src="../image/scan.png" class='scanpic'></image>
            <view class="text">
              <text>扫一扫功能</text>
            </view>
        </view>
    </view>
  </view>
</view>
