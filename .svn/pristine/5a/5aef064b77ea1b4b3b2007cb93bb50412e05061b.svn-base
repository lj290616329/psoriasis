.main{
  width:100%;
  height: 100%;  
  background:url('http://pic.tsing-tec.com/eplus/images/57a0a03105794dd6a536877b958a0a9b.png') no-repeat;  
  background-color: #ffffff;
}
.warn{
  width: 100%;
  position: fixed;
  top:0;
  background-color: red;
  font-size: 24rpx;
  color:#fff; 
  z-index:9999;
  text-align: center;
}
.personal{
  width: 100%;
  display: inline-block;
}
.headimg{
  width:190rpx;
  height: 190rpx;
  display: block;
  border-radius: 50%;  
  margin: 80rpx auto 0;
  border:6rpx #5369fd solid;
}
.nickname{
  display: block;
  margin: 15rpx auto;
  width: 100%;
  text-align: center;
  font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 50rpx;
	letter-spacing: 0px;
	color: #ffffff;
	opacity: 0.8;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.grid_box{
  width: 90%;
  text-align: center;
  
  padding-bottom: 30rpx;
  margin: 0 auto;
}
.grid{
  width: 49%;
  display: inline-block;
  line-height: 28rpx;
}
.icon{
  width: 150rpx;
	height: 150rpx;
  display: block;
  margin: 0 auto;
}
.text{
  font-size: 30rpx;
  font-weight: bold;
  color: #000000;
  opacity: 0.8;
}
.btn {
  color: #fff; 
  position: fixed;
  bottom:150rpx;
  width:350rpx;
}
.btncolor {  
  background: #5369fd;
  color: #fff;
  border: 1px solid #5369fd;
  border-radius: 35px; 
}
.report_box{
  position: relative;
  left: 180rpx;
  width: 250rpx;
  height: 250rpx;
  bottom: 150rpx;
}
.report{
  width:100%;
  height: 100%;
}


.mask{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0,0,0,0.4);
  z-index: 9999;
}
