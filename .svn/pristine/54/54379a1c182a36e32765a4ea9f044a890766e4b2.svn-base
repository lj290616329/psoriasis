<view>
  <view class="content">
    <view class="title">
      <text>风险评分历史</text>
    </view>
    <view>
      <view>
        <text class='textset1' decode='emsp'>时间</text>
        <text class='textset1' decode='emsp'>严重程度</text>
        <text class='textset1' decode='emsp'>关节炎得分</text>
        <text class='textset1' decode='emsp'>风险评估</text>
        <text class='textset1' decode='emsp'>详情</text>
      </view>
      <view wx:for="{{value}}" wx:key="{{id}}">
        <view class="blue">
          <view class='livs'>
            <text class='textset2' >{{item.cdt}}</text>
            <text class='textset' >{{item.result1}}</text>
            <text class='textset' >{{item.result2}}</text>
            <text class='textset' >{{item.result3}}</text>
            <view class="textset" id="{{item.id}}" bindtap='formSubmit'>
              <button class='button' type='primary' >二维码</button>
            </view>
          </view>  
        </view>
      </view>
    </view>
  </view>
 
  <view class='mask' wx:if='{{show}}'>
    <view class='modal-content' style='height:{{height}}'>
      <view style='width: 300px;margin:0 auto; height: 300px; background-size: 100% 100%; background-repeat: no-repeat; background-image: url({{base64ImgUrl}});'>  </view>
      <view class='modal-btn-wrapper'>
        <view class='cancel-btn' style='color:rgba(7,17,27,0.6)' bindtap='cancel'>取消</view>
        <view class='confirm-btn' style='color:#13b5f5' bindtap='confirm'>确定</view>
      </view>
    </view>
  </view>
</view>


