.mask{
 position: absolute;
 left: 0;
 right: 0;
 top: 0;
 bottom: 0;
 display: flex;
 justify-content: center;
 align-items: center;
 background-color: rgba(0,0,0,0.4);
 z-index: 9999;
}

.modal-content{
 display: flex;
 flex-direction: column;
 width: 90%;
 height: 50%;
 background-color: #fff;
 border-radius: 10rpx;
}
.modal-content image {
  width: 240px;
  margin: 0 auto
}
.modal-btn-wrapper{
 display: flex;
 flex-direction: row;
 height: 130rpx;
 line-height: 100rpx;
 border-top: 2rpx solid rgba(7,17,27,0.1);
 margin-top: 20rpx
}
.cancel-btn, .confirm-btn{
 flex: 1;
 height: 120rpx;
 line-height: 100rpx;
 text-align: center;
 font-size: 40rpx;
}

.cancel-btn{
 border-right: 2rpx solid rgba(7,17,27,0.1);
}

.main-content{
 flex: 1;
 height: 100%;
 overflow-y: hidden; 
 background-color: #fff;
}

.input{
  height: 80%;
  /* background-color: red; */
  margin-left: 33rpx;
  margin-top: 33rpx;
}
