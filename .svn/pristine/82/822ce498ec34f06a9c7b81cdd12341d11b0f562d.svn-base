.demo{
  position: relative;
  display: inline-block;
  width: 1em;
  height: 1em;
  font-size: 20px;  
  border: solid .25em #999;
  padding: .2em;
  border-radius: 0.4em;
  color:#999;
  background-color: #fff;
}
.demo::before{
  content: '';
  position: absolute;
  display: block;
  background: currentColor;
  -webkit-border-radius: .25em;
  -moz-border-radius: .25em;
  -o-border-radius: .25em;
  -ms-border-radius: .25em;
  border-radius: .25em;
  top: .6em;
  left: 0.2em;
  height: .2em;
  width: 1em;
}
.demo::after{
  top: 0.2em;
  left: .6em;
  height: 1em;
  width: .2em;
  content: '';
  position: absolute;
  display: block;
  background: currentColor;
  -webkit-border-radius: .25em;
  -moz-border-radius: .25em;
  -o-border-radius: .25em;
  -ms-border-radius: .25em;
  border-radius: .25em;
}