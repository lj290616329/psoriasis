<view class="page msg_success js_show">
  <view class="weui-msg">
    <view class="weui-msg__icon-area">
      <image src="/pages/image/error.png" style="width:300rpx;height:300rpx;margin:0 auto;"></image>
    </view>
    <view class="weui-msg__text-area">
      <view class="weui-msg__desc">{{errmsg}}</view>
    </view>
    <view class="weui-msg__opr-area">
      <view class="weui-btn-area">
        <button bindtap="back" class="weui-btn weui-btn_default" style="border-radius: 45rpx;width: 60%;">返回</button>
      </view>
    </view>
  </view>
</view>