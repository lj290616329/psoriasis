/**index.wxss**/
page {
 overflow: hidden;
 position: relative
}
.container {
  height: 100vh;
  display: flex;
  overflow: hidden;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
   background-image: url(http://1.qingke.applinzi.com/bg3.jpg);
   background-repeat:no-repeat;
   background-size:100% 100%;
   -moz-background-size:100% 100%;
} 
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.btn {
  position: absolute;
  top: 75%;  
  color: #fff
  
}
.btnleft{
  position: relative;
  border: 2px 0 2px 2px;
  border-style: solid;
  border-radius: 50%;
}
.btncolor {
  width: 150px;
  background: #5369fd;
  color: #fff;
  border: 1px solid #5369fd;
  border-radius: 35px; 
}