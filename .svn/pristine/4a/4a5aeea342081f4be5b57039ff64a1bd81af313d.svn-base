<view class="warn" wx:if='{{show}}'>{{warnmsg}}</view>
<!--index.wxml-->
<view class="container">
  <view class="userinfo">
    <view class='btn'>
      <button class='btncolor' bindtap='showAuthorize'> 开始测试 </button>
    </view>
  </view>
</view>
<view class='mask' wx:if='{{show}}'>
  <view class="logo-box">
    <image class="logo" src="/pages/image/logo.png"></image>
  </view>
  <view class='modal-content' style='height:{{height}}'>
    <view class='title1'>您还未授权</view>
    <text class='title2'>请问您是否开始授权进行测试?</text>
    <view class='modal-btn-wrapper' bindtap='hiddenAuthorize'>
        <button class='cancel-btn'  bindtap='cancel'>否</button>
        <button class='confirm-btn' open-type="getUserInfo" bindgetuserinfo="getDatas">是</button>
    </view>
  </view>
</view>