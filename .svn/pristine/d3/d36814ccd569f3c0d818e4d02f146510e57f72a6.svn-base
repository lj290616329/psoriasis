<view class='weui-title'>风险评分结果</view>
<view class="item-box">
  <view class="items">
    <view wx:for="{{list}}" wx:key="{{index}}" class="item">
      <view data-index="{{index}}" class="inner txt">
        <i class="{{item.txtStyle}}">{{item.rank}}</i>
        <image class="item-icon" mode="widthFix" src="{{item.icon}}"></image>
        <i> {{item.name}}</i>
        <span class="item-data">
          <i class="rankpace"> {{item.pace}}</i>
        </span>
      </view>
    </view>
  </view>
</view>
<view class="content2">
    <view class="buttonNext1" bindtap='getBase64ImageUrl'>
      <button type='primary' class='primary'>确定</button>
    </view>
   
</view>

  <view class='mask' wx:if='{{show}}'>
    <view class='modal-content' style='height:{{height}}'>
      <view style='width: 300px;margin:0 auto; height: 300px; background-size: 100% 100%; background-repeat: no-repeat; background-image: url({{base64ImgUrl}});'>  </view>
      <view class='text'>将此二维码展示给医生进行扫描</view>
       <view class='modal-btn-wrapper'>
        <view class='cancel-btn' style='color:rgba(7,17,27,0.6)' bindtap='cancel'>隐藏二维码</view>
        <view class='confirm-btn' style='color:#13b5f5' bindtap='confirm'>返回主页</view>
      </view>
    </view>
  </view>
<official-account class="infos"></official-account>

