.main{
  width:100%;
  height: 100%;  
  background:url('http://pic.tsing-tec.com/eplus/images/57a0a03105794dd6a536877b958a0a9b.png') no-repeat;  
  background-color: #ffffff;
}
.personal{
  width: 100%;
  display: inline-block;
}
.headimg{
  width:150rpx;
  height: 150rpx;
  display: block;
  border-radius: 50%;
  
  margin: 100rpx auto 0;

}
.nickname{
  display: block;
  margin: 0 auto;
  width: 100%;
  text-align: center;
  font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 50rpx;
	letter-spacing: 0px;
	color: #ffffff;
	opacity: 0.8;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.result_box{
  width: 90%;
  text-align: center;
  border-bottom: 4rpx dashed #1b1b1bbb;
  padding-bottom: 30rpx;
  margin: 0 auto;
}
.result{
  width: 33%;
  display: inline-block;
  line-height: 28rpx;
}
.num{
  width: 100%;
	height: 100rpx;
	font-size: 80rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #febf07;
  line-height: 100rpx; 
}
.text{
  font-size: 24rpx;
  color: #000000;
  opacity: 0.7;
}
.suggest_box{
  width: 90%;
  margin: 30rpx auto;
}
.h3{
  display: block;
  font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 40rpx;
	color: #000000;
	opacity: 0.9;
}
.suggest{
  font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 50rpx;
	display: block;
	color: #000000;
	opacity: 0.7;
}
.btn {
  color: #fff; 
  margin: -100rpx auto 100rpx;
}

.btncolor {  
  background: #5369fd;
  color: #fff;
  border: 1px solid #5369fd;
  border-radius: 35px; 
}
.report_box{
  position: relative;
  left: 180rpx;
  width: 250rpx;
  height: 250rpx;
  bottom: 150rpx;
}
.report{
  width:100%;
  height: 100%;
}




.mask{
 position: absolute;
 left: 0;
 right: 0;
 top: 0;
 bottom: 0;
 display: flex;
 justify-content: center;
 align-items: center;
 background-color: rgba(0,0,0,0.4);
 z-index: 9999;
}
.modal-content{
  display: flex;
  flex-direction: column;
  width: 70%;
  height: 420rpx;
  background-color: #fff;
  border-radius: 30rpx;

}
.logo-box{
  position: absolute;
  top: 30%;
  width: 100%;
  display: block;

}
.logo{  
  border-radius: 50%;
  width:170rpx;
  height:170rpx;
  margin: 0 auto;
  display: block;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.modal-btn-wrapper{
  display: flex;
  flex-direction: row;
  height: 100rpx;
  line-height: 100rpx;
  position: absolute;
  top: 56%;
}
.cancel-btn, .confirm-btn{
  width: 200rpx;
  height: 70rpx; 
  line-height: 70rpx;
  text-align: center;
  font-size: 32rpx;
  margin-left: 40rpx;
  margin-top: 10rpx;
  color: #000000;
  border-radius: 35rpx;
	border: solid 3rpx #000000;
	opacity: 0.4;
}
.confirm-btn {
  background-color: #5369fd;
  color: #ffffff;
  
  border: none;
  opacity: 1;

}
.title1 {
  
  margin-top: 120rpx;
  font-weight: bolder;

  text-align: center;
  
  font-size: 40rpx;
	
	font-stretch: normal;
	line-height: 60rpx;
	
	color: #000000;
	opacity: 0.9;
}
.title2 {
  text-align: center;
  font-size: 32rpx;
  margin-top: 20rpx;
  color: #ccc
}

