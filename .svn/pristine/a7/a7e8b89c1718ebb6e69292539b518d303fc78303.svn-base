// pages/pages/details.js
Page({

  /**
   * 页面的初始数据
   */
  data: {
    show1:false,
    show2:false
  },

  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {
   
    var arr = ['毫无影响', '有轻微影响', '有相当程度影响', '有十分严重的影响', '有极度影响，几乎病痛不欲生'];
    var result = JSON.parse(options.result);
    var id = result.id;
    console.log(id);
    var that = this ;
    that.setData({
      id:id
    })
    if (result.level == 0){
      result.level = '毫无影响'
    } else if (result.level == 1) {
      result.level = '有轻微影响'
    } else if (result.level == 2) {
      result.level = '有相当程度影响'
    } else if (result.level == 3) {
      result.level = '有十分严重的影响'
    } else if (result.level == 4) {
      result.level = '有极度影响，几乎病痛不欲生'
    }
    if (result.checkvalue1 == 1) {
      result.checkvalue1 = "是"
    } else {
      result.checkvalue1 = "否"
    }
    that.setData({
      checkvalue1: result.checkvalue1
    });
    if (result.checkvalue2 == 1) {
      result.checkvalue2 = "是"
    } else {
      result.checkvalue2 = "否"
    }
    that.setData({
      checkvalue2: result.checkvalue2
    })
    that.setData({
      time1: result.time1
    })
    console.log(result.time1)
    if(result.time1 == null){
      that.data.show1 = false
    }else {
      that.data.show1 = true
    }
   
    that.setData({
      time2: result.time2
    })
    if (result.time2 == null) {
      that.data.show2= false
    } else {
      that.data.show2 = true
    }
    that.setData({
      datet1: result.datet1
    })
    that.setData({
      datet2: result.datet2
    })
    that.setData({
      datet3: result.datet3
    })
    that.setData({
      datet4: result.datet4
    })
    that.setData({
      level: result.level
    })
    if (result.if_1 == 1) {
      result.if_1 = "是"
    } else {
      result.if_1 = "否"
    }
    that.setData({
      if_1: result.if_1
    })
    if (result.if_2 == 1) {
      result.if_2 = "是"
    } else {
      result.if_2 = "否"
    }
    that.setData({
      if_2: result.if_2
    })
    if (result.if_3 == 1) {
      result.if_3 = "是"
    } else {
      result.if_3 = "否"
    }
    that.setData({
      if_3: result.if_3
    })
    if (result.if_4 == 1) {
      result.if_4 = "是"
    } else {
      result.if_4 = "否"
    }
    that.setData({
      if_4: result.if_4
    })
    if (result.if_5 == 1) {
      result.if_5 = "是"
    } else {
      result.if_5 = "否"
    }
    that.setData({
      if_5: result.if_5
    })
    if (result.if_6 == 1) {
      result.if_6 = "是"
    } else {
      result.if_6 = "否"
    }
    that.setData({
      if_6: result.if_6
    })
    if (result.if_7 == 1) {
      result.if_7 = "是"
    } else {
      result.if_7 = "否"
    }
    that.setData({
      if_7: result.if_7
    })
  
  },
  toTable:function(){
    var id = this.data.id;
    console.log(id)
    wx.navigateTo({
      url: '/pages/pages/EvaluationForm5?id='+id,
    })
  },
  /**
   * 生命周期函数--监听页面初次渲染完成
   */
  onReady: function () {

  },

  /**
   * 生命周期函数--监听页面显示
   */
  onShow: function () {

  },

  /**
