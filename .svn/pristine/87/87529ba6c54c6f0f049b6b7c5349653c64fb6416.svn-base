page {
  width: 100%;
  height: 100%;
  display: fixed;
   background-repeat:no-repeat;
   background-size:200% 200%;
   -moz-background-size:200% 200%;
   background-image: url(http://1.qingke.applinzi.com/%E6%99%BA%E6%85%A7%E7%A4%BE%E5%8C%BA/bg5.jpg);
   font-family: "微软雅黑"
}
