/**index.wxss**/
page {
  background-image: url( http://1.qingke.applinzi.com/bg3.jpg);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  overflow: hidden
}
.containers {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.userinfo {
  width: 100%;
  display: flex;
  align-items: center;
  height: 240rpx;
  background: #743481;
  padding-left: 70rpx
}

.userinfo-avatar {
  width: 140rpx;
  height: 140rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}
.info {
  margin-top: 5vh;
  display: flex;
  flex-direction: row
}
.plate {
  width: 290rpx;
  height: 300rpx;
  /* border: 10rpx solid transparent; */
  /* border-color: #aaa;
  background: rgba(0, 0, 0, 0.5); */
  margin: 25rpx;
  margin-top: 60rpx;
  position: relative;
  text-align: center
}
.plate image{
  width: 130rpx;
  height:130rpx;
  display: inline-block;
  margin: 0 auto;
  margin-top: 40rpx
}
.text {
  font-size: 30rpx;
  color: white;
  font-family: "微软雅黑";
  text-align: center;
  margin-top: 30rpx
  
}
