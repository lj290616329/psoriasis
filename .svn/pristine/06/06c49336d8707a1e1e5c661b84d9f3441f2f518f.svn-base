
page{
  background-color: #7d96e0;
  height: 100%;
  display: inline-block;
}
.warn{
  width: 100%;
  position: fixed;
  top:0;
  background-color: #e75e58;
  font-size: 24rpx;
  color:#fff; 
  z-index:9999;
  text-align: center;
}
.tag{
  width:30rpx;
  height: 50rpx;
  position: absolute;
  top:40rpx;
  left:10%;
  background-color:#febf07;
  border-radius: 0 0 10rpx 10rpx; 
}
.main{
  width: 100%;
  min-height: 100%;
  background-color: #7d96e0;
}
.container{
  padding: 30rpx 10%;
  min-height:96% ;
  width:94%;
  margin: 20px auto;
  background-color: #fff;
  border-radius: 20px; 
}
.title{
  width:100%;
  color:#7d96e0;
  text-align: center;
  font-family: Arial, Helvetica, sans-serif;
  font-size: 40rpx;
  font-weight: bold;
  border-bottom: 1px #AAAAAA dotted;
  padding-bottom: 30rpx;
}
.content{
  width:100%;
}
.item{
  margin: 30rpx 0;
  float: left;
  width: 100%;
}
.item_last{
  margin: -60rpx 0 60rpx;
}
.titles{
  color:#000;
  font-size: 32rpx;
  margin-top: 5rpx;
  display: initial;
  width: inherit;
}
.lbs {
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background:#febf07;
  display: inline-block;
  margin-right: 10rpx
}

.btn {
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: normal;
  font-stretch: normal;
  line-height: 64rpx;
  letter-spacing: 0px;
  color: #7d96f4;
  width: 180rpx;
  float: right;
  border-radius: 25px;
  border: solid 1px #7d96f4;
  text-align: center;
}
.result{
  font-family: PingFangSC-Medium;
	font-weight: normal;
	font-stretch: normal;	
	letter-spacing: 0px;
	color: #5369fd;
	opacity: 0.9;
  margin-left: 5rpx;
}
.separate{
  background: #E0E3DA;
  width: 100%;
  height: 5rpx;
  float: left;
}

.btncolor {
  width: 400rpx;
  background: #5369fd;
  color: #fff;
  border: 1px solid #5369fd;
  border-radius: 35px;
  margin-top: 70rpx;
}
.hidden{
  display: none;
}
.txt {
  margin-left: 20rpx
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #7d96f4;
  z-index: 9000;
  overflow-y: scroll;
}
.pics_box{
  min-height:96% ;
  width:94%;
  margin: 20px auto;
  border-radius: 20px; 
}
.pic{
  width: 42.5%;  
  display: inline-block;
  background-color: #fff;
  margin: 15px 0% 0 5%;  
  border-radius: 30rpx;
}
.pic image{
  width: 100%;
  display: inline-block;
  margin: 30rpx 0 0;
}
.grade{
  height: 30px;
  text-align: center;
  background-color: #febf07;
  border-radius: 0 0 10px 10px;
  width: 100%;
  display: block;
  font-family: PingFangSC-Medium;
	font-size: 30rpx;
	font-weight:bold;
	font-stretch: normal;
	line-height: 30px;
	letter-spacing: 0px;
	color: #000000;
	opacity: 0.9;
}










.prompt_title{
  height: 38px;
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 38px;
  letter-spacing: 0px;
  color: #ffffff;
  width: 100%;
  text-align: center;
}









