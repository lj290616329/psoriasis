page{
  background-color: #eee;
	opacity: 0.8;
}
.main{
  width:96%;
  margin: 30rpx auto;
}
.container{
  padding:0;
}
.item_box{
  width:100%;
  background-color: #fff;
  border-radius: 30rpx;
  margin: 30rpx 0;
}
.item{
  display: inline-block;
  width:90%;
  border-bottom: 2rpx solid #eee;
  padding:0 5%;
}
.title{
  background-color: #5369fd;
  color: #fff;
  font-size: 32rpx;
  height: 98rpx;
  line-height: 98rpx;
  text-align: left;
  border-radius: 30rpx 30rpx 0 0;
  margin-top: 0;
}
.title_val{
  float: right;
}
.item_lable{  
	letter-spacing: 0px;
	color: #010101;
	opacity: 0.8;
  height: 98rpx;
  line-height: 98rpx;
}
.item_val{
  float:right;
  font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #febf07;
}
.more{
  text-align: center;
  font-size: 30rpx;
  color:#5369fd;
  height: 100rpx;
  line-height: 100rpx;
}
