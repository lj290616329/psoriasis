 .index-hd {
   padding: 80rpx;
   text-align: center;
 }
 .index-bd {
   padding: 0 30rpx 40rpx;
 }
 .index-ft {
   padding-bottom: 20rpx;
   text-align: center;
 }
 .index-logo {
   width: 86rpx;
   height: 86rpx;
 }
 .index-desc {
   margin-top: 20rpx;
   color: #888888;
   font-size: 28rpx;
 }
 .weui-cells{
   border-radius: 20rpx;
 }
 
 .navigator-box {
   opacity: 0;
   position: relative;
   background-color: #FFFFFF;
   line-height: 1.41176471;
   transform: translateY(-50%);
   transition: .3s;
 }
 .navigator-box-show {
   opacity: 1;
   transform: translateY(0);
 }
 .navigator {
   padding: 20rpx 30rpx;
   position: relative;
   display: flex;
   align-items: center;
   font-size: 30rpx;
   color: #010101;
   opacity: .8;
 }
 .navigator:before {
   content: " ";
   position: absolute;
   left: 30rpx;
   top: 0;
   right: 30rpx;
   height: 1px;
   border-top: 1rpx solid #D8D8D8;
   color: #D8D8D8;
 }
 .navigator:first-child:before {
   display: none;
 }
 .navigator-text {
   flex: 1;
 }
 .navigator-arrow {
   opacity: 1;
   position: relative;
   color: #febf07;
 }
 .kind-list-item {
   margin: 20rpx 0;
   background-color: #FFFFFF;
   border-radius: 4rpx;
   overflow: hidden;
 }
 .weui-cells:before {
   border:none;
 }
 .weui-cells:after{
   border:none;
 }
 .weui-cell {
   height: 70rpx;
   opacity: 0.8;
 }
 .weui-cell:before {
   right:15px;
 }
 .kind-list-item:first-child {
   margin-top: 0;
 }
 .kind-list-text{
   flex: 1;
 }
 .kind-list-img { 
   width: 0;
   height: 0;
   border-width: 16rpx;
   border-style: solid;
   border-color:#5369fd transparent transparent transparent;  
 }
 .kind-list-img-show{
   border-color: transparent transparent  #5369fd transparent;  
 }
 .kind-list-item-hd {
   padding: 30rpx;
   display: flex;
   align-items: center;
   transition: opacity .3s;
   font-size: 32rpx;
 }
 .kind-list-item-hd-show {
   color: #5369fd;
   position: relative;
 }
 .kind-list-item-hd-show::after {
  position: absolute;
  content: "";
  border: 2rpx solid #eee;
  width: 80%;
  left:10%;
  bottom: 0;

  
}

 .kind-list-item-bd {
   height: 0;
   overflow: hidden;
 }
 .kind-list-item-bd-show {
   height: auto;
 }
 .weui-article__section{
   padding: 20rpx 30rpx;
   border-top: 1rpx solid #D8D8D8;
 }
 .weui-article__h3{
   color: #febf07;
 }
 .weui-article__p {
   font-size: 30rpx;
   color: #000;
   opacity: 0.8;
 }

 
 .keep {
    margin-top: 50rpx;
 }
 
 .mask{
   position: fixed;
   z-index: 1000;
   top: 0;
   right: 0;
   left: 0;
   bottom: 0;
   background: rgba(0, 0, 0, 0.6);
 }
 .modal-content{
   position: fixed;
   z-index: 5000;
   top: 50%;
   left: 16px;
   right: 16px;
   -webkit-transform: translate(0, -50%);
   transform: translate(0, -50%);
   background-color: #FFFFFF;
   text-align: center;    
   overflow: hidden;
   display: -webkit-box;
   display: -webkit-flex;
   display: flex;
   -webkit-flex-direction: column;
   -webkit-box-orient: vertical;
   -webkit-box-direction: normal;
   flex-direction: column;
   max-height: 90%;
   border-radius: 30rpx;
 }
 
 .modal-btn-wrapper{  
   display: flex;
   margin-bottom: 15px;
 }
 .img_box{
   -webkit-box-flex: 1;
   -webkit-flex: 1;
   flex: 1;
   overflow-y: auto;
   -webkit-overflow-scrolling: touch;   
 }
 .img_box image{
   width: 100%;
 }
 .cancel-btn, .confirm-btn{
   width: 35%;
   height: 70rpx; 
   line-height: 70rpx;
   text-align: center;
   font-size: 32rpx;
   
   
   color: #000000;
   border-radius: 35rpx;
   border: solid 3rpx #000000;
   opacity: 0.4;
 }
 .confirm-btn {
   background-color: #5369fd;
   color: #ffffff;  
   border: none;
   opacity: 1;
 
 }
 .title1 {  
   margin-top: 20rpx;
   font-weight: bolder;
   text-align: center;  
   font-size: 40rpx;	
   font-stretch: normal;
   line-height: 40rpx;	
   color: #000000;
   opacity: 0.6;
 }
