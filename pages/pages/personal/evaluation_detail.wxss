.navigator-box {
  opacity: 0;
  position: relative;
  background-color: #FFFFFF;
  line-height: 1.41176471;
  transform: translateY(-50%);
  transition: .3s;
}
.navigator-box-show {
  opacity: 1;
  transform: translateY(0);
}
.navigator {
  padding:30rpx;
  position: relative;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #010101;
  opacity: .8;
}
.navigator:before {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 30rpx;
  height: 1px;
  border-top: 1rpx solid #D8D8D8;
  color: #D8D8D8;
}
.navigator:first-child:before {
  display: none;
}
.navigator-text {
  flex: 1;
}
.navigator-arrow {
  opacity: 1;
  position: relative;
  color: #febf07;
}
.kind-list-item {
  margin: 20rpx 0;
  background-color: #FFFFFF;
  border-radius: 4rpx;
  overflow: hidden;
}
.weui-cells:before {
  border:none;
}
.weui-cells:after{
  border:none;
}
.weui-cell {
  height: 70rpx;
  opacity: 0.8;
}
.weui-cell:before {
  right:15px;
}
.kind-list-item:first-child {
  margin-top: 0;
}
.kind-list-text{
  flex: 1;
}
.kind-list-img { 
  width: 0;
  height: 0;
  border-width: 16rpx;
  border-style: solid;
  border-color:#5369fd transparent transparent transparent;  
}
.kind-list-img-show{
  border-color: transparent transparent  #fff transparent;  
}
.kind-list-item-hd {
  padding: 30rpx;
  display: flex;
  align-items: center;
  transition: opacity .3s;
  font-size: 30rpx;
}
.kind-list-item-hd-show {
  background-color: #5369fd;
  color:#fff;
}
.kind-list-item-bd {
  height: 0;
  overflow: hidden;
}
.kind-list-item-bd-show {
  height: auto;
  padding-bottom: 30rpx;
}
.weui-article__section{
  padding: 20rpx 30rpx;
  border-top: 1rpx solid #D8D8D8;
  margin-bottom: 0;
}
.weui-article__h3{
  font-weight: bold;
  position: relative;
  padding-left: 20rpx;
}
.weui-article__h3::before{
  position: absolute;
  left: 0;
  content: '';
  background-color: #febf07;
  width: 16rpx;
  border-radius: 50%;
  height: 16rpx;
  top: 10rpx;
}
.weui-article__h4{
  color: #febf07;
}
.weui-article__p {
  font-size: 30rpx;
  color: #000;
  opacity: 0.8;
  margin: 0;
  padding-left: 20rpx;
}
.weui-cells__title:before{
  content: " ";
  position: absolute;
  top: 0;
  right: 15px;
  height: 1px;
  border-top: 1rpx solid #d9d9d9;
  color: #d9d9d9;
  left: 15px;
}

.weui-cells__title{
  background-color: #fff;
  margin: 0;
  padding-top: 0.77em;
  padding-bottom:0.3em;
  font-size: 17px;
  color: #010101;
  opacity: 0.8;
  position: relative;
}

.textarea-wrp {
  padding: 0 25rpx;
  background-color: #fff;
  color: #999;

}
.weui-textarea{
  padding: 0 0 20rpx 0;
}

