page{
  height:100%;
}
.main{
  width:100%;
  height: 100%;  
  background:url('http://pic.tsing-tec.com/eplus/images/57a0a03105794dd6a536877b958a0a9b.png') no-repeat;  
  background-color: #ffffff;
}
.warn{
  width: 100%;
  position: fixed;
  top:0;
  background-color: red;
  font-size: 24rpx;
  color:#fff; 
  z-index:9999;
  text-align: center;
}
.personal{
  width: 100%;
  display: inline-block;
}
.headimg{
  width:190rpx;
  height: 190rpx;
  display: block;
  border-radius: 50%;  
  margin: 80rpx auto 0;
  border:6rpx #5369fd solid;
}
.nickname{
  display: block;
  margin: 15rpx auto;
  width: 100%;
  text-align: center;
  font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 50rpx;
	letter-spacing: 0px;
	color: #ffffff;
	opacity: 0.8;
}
.container {
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.grid_box{
  width: 90%;
  text-align: center;
  
  padding-bottom: 30rpx;
  margin: 0 auto;
}
.grid{
  width: 49%;
  display: inline-block;
  line-height: 28rpx;
}
.icon{
  width: 150rpx;
	height: 150rpx;
  display: block;
  margin: 0 auto;
}
.text{
  font-size: 30rpx;
  font-weight: bold;
  color: #000000;
  opacity: 0.8;
}
.btn {
  color: #fff; 
  position: fixed;
  bottom:150rpx;
  width:350rpx;
}
.btncolor {  
  background: #5369fd;
  color: #fff;
  border: 1px solid #5369fd;
  border-radius: 35px; 
}
.report_box{
  position: relative;
  left: 180rpx;
  width: 250rpx;
  height: 250rpx;
  bottom: 150rpx;
}
.report{
  width:100%;
  height: 100%;
}


.mask{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0,0,0,0.4);
  z-index: 9999;
}
.copyright{
  position:relative;
  bottom:60rpx;
  text-align: center;
  font-size: 24rpx;
  color: #000;
}
/* 弹出框*/
.mask{
  position: fixed;
  z-index: 10;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
}

.modal-content{
  position: fixed;
  z-index: 5000;
  top: 50%;
  left: 0;
  right: 0;
  width:80%;
  margin:0 auto;
  -webkit-transform: translate(0, -50%);
  transform: translate(0, -50%);
  background-color: #FFFFFF;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-direction: column;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  flex-direction: column;
  max-height: 90%;
  border-radius: 30rpx;
}
.logo-box{
  position: absolute;
  top: -85rpx;
  width: 100%;
  display: block;
  z-index:5001;
}
.logo{  
  border-radius: 50%;
  width:170rpx;
  height:170rpx;
  margin: 0 auto;
  display: block;
  z-index:5001;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.modal-btn-wrapper{  
  display: flex;
  margin: 25rpx 0;
}
.img_box{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;   
}
.img_box image{
  width: 100%;
}
.cancel-btn, .confirm-btn{
  width: 35%;
  height: 70rpx; 
  line-height: 70rpx;
  text-align: center;
  font-size: 32rpx;
  color: #000000;
  border-radius: 35rpx;
	border: solid 3rpx #000000;
	opacity: 0.4;
}
.confirm-btn {
  background-color: #5369fd;
  color: #ffffff;  
  border: none;
  opacity: 1;
}
.title1 {  
  margin-top: 95rpx;
  font-weight: bolder;
  text-align: center;  
  font-size: 40rpx;	
	font-stretch: normal;
	line-height: 40rpx;	
	color: #000000;
  opacity: 0.8;
  margin-bottom: 10px;
}
.title2{ 
  text-align: center;  
  font-size: 34rpx;	
	font-stretch: normal;
	line-height: 34rpx;	
	color: #000000;
	opacity: 0.6;
  margin: 15rpx auto;
  padding: 0 25rpx;
}

.title3{ 
  text-align: left;  
  font-size: 30rpx;	
	font-stretch: normal;
	line-height: 44rpx;	
	color: #000000;
	opacity: 0.6;
  padding: 0 25rpx;
  display: block;
}
